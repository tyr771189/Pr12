<UserControl x:Class="Interface.Elements.Users"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Interface.Elements"
             mc:Ignorable="d" MouseUp="SelectUsers" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="50" Background="Black" Margin="5">
        <Border CornerRadius="15" Width="30" Height ="30" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Border.Background>
                <ImageBrush ImageSource="/Images/pngwing.com.png"/>
            </Border.Background>
        </Border>
        <TextBlock x:Name="FIO" Margin="45,5,5,5" Padding="5" TextWrapping="Wrap" Text="Иванов Иван" Foreground="White"/>
    </Grid>
</UserControl>
